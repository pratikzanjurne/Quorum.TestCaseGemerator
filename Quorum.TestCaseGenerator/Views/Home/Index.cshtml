@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Shapes and Line Toolbox</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>
    <div id="container">
        <div id="toolbox">
            <div id="card" class="shape"></div>
            <div id="diamond" class="shape"></div>
            <div id="rectangle" class="shape"></div>
            <div id="triangle" class="shape"></div>
            <div id="circle" class="shape"></div>
            <div id="line" class="line"></div> <!-- New Line Element -->
        </div>
    </div>

    <script src="script.js"></script>
</body>
</html>
